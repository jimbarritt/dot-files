[user]
	name = Jim Barritt
	email = jim@planet-ix.com

[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto

[alias]
        sm = submodule
	st = status
	br = branch
        serve = !git daemon --reuseaddr --verbose  --base-path=. --export-all ./.git
        lol = log --graph --decorate --pretty=oneline --abbrev-commit
[push]
	default = matching

